{
  "name": "@nebula-web-app/app",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "dev": "multiplerun dev",
    "app:dev": "vite",
    "app:dev:browser": "HTTPS=true PORT=3000 TS_NODE_PROJECT=tsconfig.script.json node -r ts-node/register -r tsconfig-paths/register scripts/start.ts",
    "start": "yarn run app:dev",
    "build": "tsc --noEmit && vite build",
    "pack": "rocket-punch build",
    "publish": "rocket-punch publish"
  },
  "multiplerun": {
    "dev": [
      "yarn run app:dev",
      [
        "tsc --watch",
        {
          "command": "yarn run app:dev:browser",
          "wait": "5000"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "../.eslintrc.js"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!**/*.d.ts?(x)",
      "!**/*.stories.ts?(x)",
      "!**/__*__/**"
    ]
  },
  "browserslist": {
    "production": [
      "chrome > 60",
      "firefox > 60",
      "safari > 12"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@rx-stream/pipe": "^0.7.0",
    "@rx-stream/react": "^0.7.0",
    "@sentry/react": "^6.5.1",
    "@terra-dev/patch-react-query-focus-refetching": "^0.15.0",
    "@terra-dev/use-browser-inactive": "^0.15.0",
    "@terra-dev/use-google-analytics": "^0.15.0",
    "@terra-dev/use-interval": "^0.15.0",
    "@terra-dev/use-router-scroll-restoration": "^0.15.0",
    "@terra-money/terra.js": "^1.8.0",
    "@terra-money/wallet-provider": "^0.15.0",
    "@terra-money/webapp-provider": "^0.15.0",
    "big.js": "^6.1.1",
    "graphql": "^15.5.0",
    "puppeteer": "^10.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.16.0",
    "react-responsive": "^8.2.0",
    "react-router-dom": "^5.2.0",
    "react-spinners-kit": "^1.9.1",
    "react-use-clipboard": "^1.0.7",
    "rxjs": "^7.1.0",
    "styled-components": "^5.3.0",
    "use-resize-observer": "^7.0.0",
    "vite-plugin-svgr": "^0.2.0",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/big.js": "^6.1.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/numeral": "^2.0.1",
    "@types/puppeteer": "^5.4.3",
    "@types/ramda": "^0.27.40",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/react-is": "^17.0.0",
    "@types/react-responsive": "^8.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@types/testing-library__user-event": "^4.2.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "multiplerun": "^2.0.1",
    "rocket-punch": "^2.5.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.5",
    "vite-tsconfig-paths": "^3.3.13"
  }
}
