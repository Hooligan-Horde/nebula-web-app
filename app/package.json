{
  "name": "@nebula-web-app/app",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "app:dev": "vite",
    "app:dev:browser": "HTTPS=true PORT=3000 TS_NODE_PROJECT=tsconfig.script.json node -r ts-node/register -r tsconfig-paths/register scripts/start.ts",
    "start": "multiplerun dev",
    "build": "tsc --noEmit && vite build",
    "pack": "rocket-punch build",
    "publish": "rocket-punch publish",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "test": "react-scripts test --colors",
    "coverage": "react-scripts test --colors --coverage",
    "precommit": "lint-staged"
  },
  "multiplerun": {
    "dev": [
      "yarn run tsc --watch --noEmit",
      [
        "yarn run app:dev",
        {
          "command": "yarn run app:dev:browser",
          "wait": "5000"
        }
      ]
    ]
  },
  "lint-staged": {
    "src/{**/*,*}/*.{ts,tsx}": [
      "cross-env CI=true react-scripts test --colors --findRelatedTests"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!**/*.d.ts?(x)",
      "!**/*.stories.ts?(x)",
      "!**/__*__/**"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@rx-stream/pipe": "^0.7.0",
    "@rx-stream/react": "^0.7.0",
    "@sentry/react": "^6.7.2",
    "@storybook/addon-storysource": "^6.3.1",
    "@storybook/addons": "^6.3.1",
    "@terra-dev/big-interpolate": "^0.17.1",
    "@terra-dev/browser-check": "^1.0.0",
    "@terra-dev/patch-react-query-focus-refetching": "^0.17.1",
    "@terra-dev/use-browser-inactive": "^0.17.1",
    "@terra-dev/use-dialog": "^0.17.1",
    "@terra-dev/use-google-analytics": "^0.17.1",
    "@terra-dev/use-interval": "^0.17.1",
    "@terra-dev/use-restricted-input": "^0.17.1",
    "@terra-dev/use-router-scroll-restoration": "^0.17.1",
    "@terra-money/terra.js": "^1.8.4",
    "@terra-money/wallet-provider": "^1.0.0",
    "@terra-money/webapp-provider": "^0.17.1",
    "big.js": "^6.1.1",
    "chart.js": "^3.4.0",
    "chartjs-plugin-datalabels": "^2.0.0-rc.1",
    "color": "^3.1.3",
    "d3-ease": "^3.0.1",
    "d3-timer": "^3.0.1",
    "date-fns": "^2.22.1",
    "fix-hmr": "^1.0.2",
    "graphql": "^15.5.1",
    "numeral": "^2.0.6",
    "puppeteer": "^10.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-masonry-css": "^1.0.16",
    "react-query": "^3.18.0",
    "react-responsive": "^8.2.0",
    "react-router-dom": "^5.2.0",
    "react-spinners-kit": "^1.9.1",
    "react-use-clipboard": "^1.0.7",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^7.1.0",
    "styled-components": "^5.3.0",
    "use-resize-observer": "^7.0.0",
    "web-vitals": "^2.0.1"
  },
  "devDependencies": {
    "@mdx-js/react": "^1.6.22",
    "@peculiar/webcrypto": "^1.1.7",
    "@storybook/addon-actions": "^6.3.1",
    "@storybook/addon-docs": "^6.3.1",
    "@storybook/addon-essentials": "^6.3.1",
    "@storybook/addon-links": "^6.3.1",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.3.1",
    "@storybook/theming": "^6.3.1",
